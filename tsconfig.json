{
    "compilerOptions": {
        "outDir": "dist",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@store/*": ["src/store/*"],
            "@components/*": ["src/components/*"],
            "@scenes/*": ["src/scenes/*"],
            "@style/*": ["src/style/*"]
        }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
